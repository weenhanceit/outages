<div class="test-outages-day">
  <%= calendar(number_of_days: 1,
      events: @outages,
      start_date: Time.zone.parse(params[:earliest]),
      open_closed: true) do |date, outages| %>
  <%= date %>
  <ul class="list-unstyled">
    <% outages.each do |outage| %>
    <li>
      <%= detailed_calendar_outage(outage, date) %>
    </li>
    <% end %>
  </ul>
  <% end %>
</div>
